.switch-demo-page,page{
    background: #f2f2f2;
    height: 100%;
}
/**swipe cell**/
.hanzo-swipe-cell__left,
.hanzo-swipe-cell__right {
    display: inline-block;
    width: 130rpx;
    font-size: 32rpx;
    line-height: 88rpx;
    height: 88rpx;
    color: #fff;
    text-align: center;
    background-color: #f44;
    position: absolute;
    top: 0;
}

.hanzo-swipe-cell__left {
     left: 0;
     transform: translate3d(-100%, 0, 0);
 }

.hanzo-swipe-cell__right {
     right: 0;
     transform: translate3d(100%, 0, 0);
 }

