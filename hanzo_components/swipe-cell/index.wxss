@charset "utf-8";
.hanzo-swipe-cell {
  position: relative;
  overflow: hidden;
}
.hanzo-swipe-cell__left,
.hanzo-swipe-cell__right {
  position: absolute;
  top: 0;
  height: 100%;
}
.hanzo-swipe-cell__left {
  left: 0;
  -webkit-transform: translate3d(-100%, 0, 0);
          transform: translate3d(-100%, 0, 0);
}
.hanzo-swipe-cell__right {
  right: 0;
  -webkit-transform: translate3d(100%, 0, 0);
          transform: translate3d(100%, 0, 0);
}
