.lovesbox {
	position: absolute;
	top: 180rpx;
	right: 106rpx;
	z-index: 9;
}

.praiseScale {
	animation: lovezoom .8s ease-out;
}

.aimateloves {
	position: absolute;
	top:0;
	left: 0;
	opacity: 0;
	transform: scale(0.1,0.1);
}

.paopao {
	opacity: 0.5;
	animation: loverise .8s linear infinite;
}

@keyframes loverise {
    0% { 
		transform: scale(0.5,0.5) translate(0,0) ;
	}
  	100% { 
		opacity: 0;
		transform: scale(1.5,1.5) translate(5rpx,-100rpx);
	}
}

@keyframes lovezoom {
    0% { 
		transform: scale(1,1);
	}
    50% { 
		transform: scale(1.5,1.5);
	}
    100% { 
		transform: scale(1,1);
	}
}