
.global-fade-in {
    animation: fadeIn 0.5s both;
}
.vendor-name{
    font-size:26rpx;
    text-align:center;
    padding-top:15rpx;
    font-weight: 400;
    font-family: PingFangSC-Regular;
    color:#555555;
}

.hanzo-product-card {
    width: 710rpx;
    margin: 20rpx auto;
    border-radius: 8rpx;
    background: #fff;
    overflow: hidden;
}
.hanzo-product-card>.row{
    margin: 0rpx 20rpx;
    position: relative;
}
/*商品标题*/
.product-title {
    display: flex;
    justify-content: space-between;
    border-bottom: 1rpx solid #E9EAEB;
    padding: 20rpx 0;
}
.product-title.type-fresh-house{
    border-bottom:none;
    padding: 20rpx 0 0 0;
}
.product-title .title {
    font-size: 32rpx;
    color: #323233;
    font-weight: bold;
    line-height: 45rpx;
    text-overflow: ellipsis;
    max-height: 84rpx;
    min-width: 520rpx;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    overflow: hidden;
    padding-right: 20rpx;
}
.product-title.type-fresh-house .title {
    -webkit-line-clamp: 1;
}
.product-title .split{
    border-left: 1px solid #f2f2f2;
}
.product-title .follows {
    width: 140rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    min-width: 140rpx;
    padding-left: 10rpx;
}
.product-title .follows .number {
    font-size:28rpx;
    color:#ff7625;
    overflow:hidden;
    text-overflow:ellipsis;
    white-space:nowrap;
    display:block;
}
.product-title .follows .text {
    font-size: 26rpx;
    color: #999999;
    display: block;
}

/*购买规则*/
.buy-rule{
    padding: 20rpx 0 10rpx;
    font-weight: 500;
}
.buy-rule.type-fresh-house{
    padding-top: 10rpx;
}
.buy-rule .row {
    display: flex;
    justify-content: space-between;
    font-size: 26rpx;
    font-weight: 500;
    margin-bottom: 2rpx;
}
.buy-rule .yellow {
    color: #FF7320;
}
.buy-rule .red {
    color: #E60012;
}
.buy-rule .grey {
    color: #555555;
}
.buy-rule .sale-data {
    display: flex;
    justify-content: flex-end;
    align-items: top;
    font-weight: normal;
    padding-right: 10rpx;
}
.buy-rule .pretime {
    text-align: center;
    display: flex;
    flex-wrap: wrap;
    align-items: center;
    flex-direction: column;
    justify-content: center;
    font-weight: 500;
}
.buy-rule .thtime {
    line-height: 37rpx;
}

.buy-rule .salenum{
    display: flex;
    /*justify-content: flex-end;*/
    align-items: center;
    color: #555555;
    font-size: 28rpx;
    font-weight: 600;
    padding-right: 2rpx;
}
.buy-rule .salenum .cut{
    margin: 0 5rpx;
}

/* 加购按钮 */
.cart-button{
    height: 74rpx;
    width: 262rpx;
    position: relative;
}
.button-cover{
    width: 100%;
    height: 100%;
    line-height: 74rpx;
    border-radius: 42rpx;
    font-size: 28rpx;
    color: #fff;
    text-align: center;
    font-weight: 400;
    position: absolute;
    /*font-family: PingFangSC-Regular;*/
}
.button-cover::after{
    border: none;
    outline: none;
}
.button-cover.waiting{
    background: #21C0AD;
}
.button-cover.active{
    background: #E60012;
}
.button-cover.fresh-house{
    background: #FF7320;
}
.button-cover.over{
    background: #D8D8D8;
    pointer-events: none;
}
.button-cover.soldout{
    background: #D8D8D8;
    pointer-events: none;
}
.button-cover.lack{
    background: #D8D8D8;
    pointer-events: none;
}

/* 价签样式 */
.price-and-buy{
    padding-bottom: 20rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
}
.price-and-buy .price {
    font-size: 82rpx;
    line-height: 1;
}

.price-and-buy .price .big {
    display: inline-block;
    font-size: 54rpx;
    font-weight: bold;
    line-height: 75rpx;
    color: #E60012;
}
.price-and-buy .price .small {
    display: inline-block;
    margin-left: 12rpx;
    font-size: 28rpx;
    font-weight: normal;
    line-height: 40rpx;
    text-decoration: line-through;
    color: #999999;
}
.price-and-buy .price .big::before{
    content: '¥';
    margin-right: 10rpx;
    font-size: 32rpx;
    font-weight: normal;
}
.price-and-buy .price .small::before{
    content: '¥';
    font-size: 28rpx;
    font-weight: normal;
}

/* 消费者头像样式 */
.product-buyer {
    display: flex;
    justify-content: space-between;
    border-top: 1rpx solid #E9EAEB;
    position: relative;
    height: 90rpx;
    margin:0rpx 20rpx 0 20rpx;
    align-items: center;
}
.product-buyer .dall{
    display:flex;
    align-items:center;
}

.product-buyer .dall .avator{
    width: 42rpx;
    height: 42rpx;
    border-radius: 100%;
    border: 1px solid #fff;
    margin-right: -10rpx;
}
.product-buyer .dall .more{
    color: #ffffff;
    display: inline-block;
    border-radius: 50%;
    border: 1px solid #fff;
    width: 42rpx;
    height: 42rpx;
    vertical-align: middle;
    position: relative;
    background-image: url("data:image/png;base64,");
    background-repeat: no-repeat;
    background-position: center center;
    background-size: contain;
}
.product-buyer .dall .em{
    font-size: 26rpx;
    font-style: normal;
    color: #999999;
    margin-left: 15rpx;
}
/*商品封面展示*/
.product-cover {
    width: 690rpx;
    height: 388rpx;
    position: relative;
    margin: 10rpx auto 0;
    z-index: 0;
}
.product-cover.fresh-house{
    width: 100%;
    height: 400rpx;
    margin: 0;
}

.product-cover .cover {
    width: 100%;
    height: 100%;
    display: block;
    position: relative;
}

.product-cover .video {
    width: 100%;
    height: 100%;
    display: block;
}

.cover-mask{
    position: absolute;
    z-index: 10;
    width:100%;
    display:flex;
    justify-content:center;
    align-items: center;
    color:#fff;
    background-position:center;
    background-size:cover;
    background-image: url(data:image/png;base64,);
}

/* 缺货遮罩 */
.cover-mask.lack{
    background:rgba(230,0,18,0.45);
    height:80rpx;
    font-size:30rpx;
    bottom:0;
    left:0;
    font-weight:900;
}
/* 售罄遮罩 */
.cover-mask.soldout{
    top: 0;
    left: 0;
    height:100%;
    font-size:50rpx;
}
/**视频图标**/
.video-play-button{
    position: absolute;
    width: 132rpx;
    height: 132rpx;
    top: 34%;
    left: 289rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    z-index: 100;
    background: rgba(255,255,255,0.8);
    border-radius: 100%;
    box-shadow:0 0 10rpx 0 #aaa;
}
.video-player-icon{
    display: block;
    margin: 0 auto;
    width:0;
    height:0;
    border-top:28rpx solid transparent;
    border-bottom:28rpx solid transparent;
    border-left:40rpx solid #8D8D8D;
    position: relative;
    left: 5rpx;
}
/* 骨架页样式 */
.skeleton{
    height: 610rpx;
    box-sizing: border-box;
    padding: 20rpx;
}
.skeleton .view{
    background-color: #e9edf1;
}
.skeleton .cover{
    width:100%;
    height:350rpx;

}
.skeleton .title{
    height: 30rpx;
    width: 400rpx;
    margin: 15rpx 0;
}
.skeleton .sub-title{
    height: 25rpx;
    width: 300rpx;
    margin: 15rpx 0;
}

.skeleton .line{
    height: 20rpx;
    width: 200rpx;
    margin: 10rpx 0;
}

.skeleton .bottom{
    height:54rpx;
    width:650rpx;
    margin-top: 20rpx;
}

